{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Node",
		"strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
  },
	"include": [ "db", "src", "types"]
}